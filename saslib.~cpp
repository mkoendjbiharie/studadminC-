//---------------------------------------------------------------------------


#pragma hdrstop
#include <vcl.h>

//#include "string.h"
#include "saslib.h"

//---------------------------------------------------------------------------

#pragma package(smart_init)

String & rearrange(String &datum)
{
   datum = datum.SubString(4, 4) + datum.SubString(2,2) + datum.SubString(0,2);
   return datum;
}